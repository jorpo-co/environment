{
    "name": "jorpo/environment",
    "description": "Environment Management in an OOP way",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@designermonkey.co.uk"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "josegonzalez/dotenv": "^3.2",
        "myclabs/deep-copy": "^1.9",
        "php": "^7.4 || ^8.0",
        "php-ds/php-ds": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Jorpo\\Environment\\": [
                "src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Jorpo\\Environment\\": [
                "tests"
            ]
        }
    }
}
