{
    "name": "jorpo/environment",
    "description": "Environment Management in an OOP way",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@designermonkey.co.uk"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "josegonzalez/dotenv": "^3.2",
        "myclabs/deep-copy": "^1.10",
        "php": "^8.0",
        "php-ds/php-ds": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Jorpo\\Environment\\": [
                "src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "vimeo/psalm": "^4.13"
    },
    "autoload-dev": {
        "psr-4": {
            "Jorpo\\Environment\\": [
                "tests"
            ]
        }
    }
}
